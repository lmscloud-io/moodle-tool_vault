{{!
    @template tool_vault/backup_details

    Example context (json):
    {
        "sectionurl": "#",
        "title": "Title",
        "details": "Details"
    }

}}
{{#sectionurl}}
    <p>
        <a href="{{sectionurl}}">Back to Site backup</a>
    </p>
{{/sectionurl}}

<h4>{{{title}}}</h4>

<table class="generaltable">
    {{#metadata}}
        <tr>
            <th>{{name}}</th>
            <td>{{{value}}}</td>
        </tr>
    {{/metadata}}
</table>

<h4>Logs</h4>

<pre>
{{{logs}}}
</pre>
